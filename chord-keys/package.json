{
  "name": "chord-keys",
  "version": "0.1.0",
  "type": "module",
  "description": "Advanced keyboard shortcuts library for React with built-in conflict detection and discoverable shortcuts",
  "main": "./src/index.ts",
  "module": "./src/index.ts",
  "types": "./src/index.ts",
  "exports": {
    ".": {
      "types": "./src/index.ts",
      "development": "./src/index.ts",
      "default": "./src/index.ts"
    },
    "./style.css": "./src/index.css",
    "./dist": {
      "import": "./dist/chord-keys.js",
      "require": "./dist/chord-keys.umd.cjs",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "vite",
    "build": "tsc --p tsconfig.build.json && vite build",
    "lint": "eslint .",
    "test": "vitest"
  },
  "dependencies": {
    "@base-ui-components/react": "^1.0.0-beta.4",
    "lucide-react": "^0.546.0"
  },
  "peerDependencies": {
    "react": "^18.0.0 || ^19.0.0",
    "react-dom": "^18.0.0 || ^19.0.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.36.0",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "^24.6.0",
    "@types/react": "^19.1.16",
    "@types/react-dom": "^19.1.9",
    "@vitejs/plugin-react": "^5.0.4",
    "@vitest/browser": "^3.2.4",
    "babel-plugin-react-compiler": "^19.1.0-rc.3",
    "eslint": "^9.36.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.22",
    "globals": "^16.4.0",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.45.0",
    "vite": "npm:rolldown-vite@7.1.14",
    "vitest": "^3.2.4",
    "vitest-browser-react": "^1.0.1"
  },
  "overrides": {
    "vite": "npm:rolldown-vite@7.1.14"
  },
  "keywords": [
    "react",
    "keyboard",
    "shortcuts",
    "hotkeys",
    "keybindings",
    "accessibility"
